<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_15"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_15"
        android:supportsRtl="true"
        android:theme="@style/Theme._15"
        tools:targetApi="31">
        <activity
            android:name=".ui.Profile"
            android:exported="false" />
        <activity
            android:name=".ui.Game5x5"
            android:exported="false" />
        <activity
            android:name=".ui.Game4x4"
            android:exported="false" />
        <activity
            android:name=".ui.Game3x3"
            android:exported="false" />
        <activity
            android:name=".management.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".content.TimerService" />
    </application>

</manifest>